{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/forbid-prop-types": 0,
      "object-curly-newline": [1, {"multiline": true}]
    },
    "env": {
      "browser": true
    }
  }